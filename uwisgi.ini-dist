[uwsgi]
chdir=/srv/evalajanda/
module=django_wsgi:application
env=DJANGO_SETTINGS_MODULE=settings
master=True
pidfile=/var/run/fubral.pid
socket=127.0.0.1:49152
processes=5
uid=1000
gid=1000
harakiri=20
limit-as=512
max-requests=5000
vacuum=True
home=/srv/envs/evalajanda/
daemonize=/var/log/uwsgi/fubral.log
